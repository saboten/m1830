<!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<%= content_tag :body,
    id: params[:controller] + "_" + params[:action],
    data: {command_url: command_game_session_path(id: @game.id), score_url: final_score_game_session_path(id: @game.id)} do -%>

<%= yield %>

<% end -%>
</html>
